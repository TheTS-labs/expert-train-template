{
  "name": "expert-train-template",
  "version": "1.0.0",
  "description": "Webpack template for a quick start (TypeScript, React, Jest)",
  "private": true,
  "repository": "git@github.com:TheTS-labs/expert-train-template.git",
  "author": "Roman <romantovt31@gmail.com>",
  "license": "GNU GPLv3",
  "scripts": {
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --config jest.config.ts --watch",
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:dev:serve": "webpack serve --progress --config webpack.dev.js",
    "lint": "yarn run eslint --ext .ts,.tsx,.js,.jsx . --ignore-pattern dist",
    "lint:fix": "yarn run eslint --ext .ts,.tsx,.js,.jsx . --fix --ignore-pattern dist",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\""
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@hot-loader/react-dom": "^17.0.2",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.185",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "depcheck": "^1.4.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "prettier": "^2.7.1",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.1",
    "fp-ts": "^2.12.3",
    "io-ts": "^2.2.18",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.0"
  }
}
